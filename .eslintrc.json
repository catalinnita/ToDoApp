{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "no-var": 1,
    "prefer-const": 1,
    "no-labels": 1,
    "keyword-spacing": 1,
    "react/no-unused-prop-types": 0,
    "react/jsx-filename-extension": 1,
    "no-empty-label": 0,
    "space-after-keywords": 0,
    "space-return-throw-case": 0,

    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,

    "linebreak-style": 0,

    "class-methods-use-this": 0
  }
}